        <Label layoutX="23.0" layoutY="63.0" text="Login" />
        <Label layoutX="20.0" layoutY="102.0" text="Password" />
        <PasswordField fx:id="passwordField" layoutX="99.0" layoutY="98.0" prefHeight="26.0" prefWidth="174.0" />
